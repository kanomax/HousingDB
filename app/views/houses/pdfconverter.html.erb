<div class="page-header"><h1 class="text-center">Pdf Upload</h1></div>
<br \>
<%= form_tag url_for(:action => 'pdfupload'), :multipart => true, :class => "form-horizontal" do %>
    <div class="row">
      <div class="col-md-4">
        <%= label_tag 'datafile', 'Pdf File:'%> <%= file_field_tag "datafile", accept: "pdf", multiple: true,class: "form-control" %>
      </div>
      <div class="col-md-4">
      <%= label_tag 'county', 'GIS County:'%> <%= text_field_tag "county","", class: "form-control"  %>
      </div>
    </div><br \>
    <div class="row"></div>
      <%= check_box_tag 'sales_only',true,false %> <%= label_tag 'sales_only', 'Upload Sales Only'%><br /><br />

      <%= submit_tag "Upload", class: "btn btn-primary" %>

<% end %>   

