<div class="page-header"><h2 class="text-center">Update House Status</h2></div>
<%= simple_form_for @house do |f| %>
<%= f.error_notification %>

  			<div class="form-inputs">
				<%= f.input :status, collection: [["Unknown"],["Listed"], ["Sold"], ["Pending"]], :include_blank => false, :class =>'btn-lg'%><br /><br /> 

  		<%= f.button :submit, :class => "btn btn-primary" %>
  		<%= link_to "Cancel", :back, :class => "btn btn-danger" %>
  		<% end %>
</div>