<%= simple_form_for([@house, @listing]) do |f| %>

<%= f.error_notification %>

  <div class="form-actions">
    <%= f.input :listingprice, label: "Listing Price", :min => 0 %><br /><br />
    <%= f.input :listingdate, label: "Listing Date", :start_year => (Time.now.year - 40), :end_year => Time.now.year, :include_blank => true%><br /><br />
    <%= f.hidden_field :agent_id %>
      <div class="row">
        <div class="col-lg-6">
          <div class="input-group">
      <span class="input-group-btn">
        <%= link_to_function "Select Agent", 'select_agent_fields()', :class => "btn btn-primary" %>
      </span>
            <input type="text" class="form-control" id="agent_attributes_name" readonly value='<%= @agentname %>'>
          </div><!-- /input-group -->
        </div><!-- /.col-lg-6 -->
</div>





      <div class="panel-body">
        <div class="row">
          <div class="col-md-4">
            <h3 id=>
              </h3>
          </div>
          <div class="col-md-4">
            <h3></h3>
          </div>

        </div>
      </div>
    </div>
	<label></label>
    <br />
    <%= render('layouts/agent_fields', :f => f) %>
    <%= f.submit "Save Listing Information", :class => "btn btn-primary" %>
  </div>


<% end %>
